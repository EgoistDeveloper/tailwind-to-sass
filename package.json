{
  "name": "@egoistdeveloper/twcss-to-sass",
  "version": "1.0.3",
  "description": "HTML template to SASS converter for TailwindCSS",
  "source": "src/index.js",
  "main": "dist/twcss-to-sass.js",
  "type": "commonjs",
  "author": "EgoistDeveloper",
  "license": "MIT",
  "homepage": "https://github.com/EgoistDeveloper/twcss-to-sass#readme",
  "bugs": {
    "url": "https://github.com/EgoistDeveloper/twcss-to-sass/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:browser": "webpack && cpx dist/twcss-to-sass.js examples/browser",
    "build:node": "webpack && cpx dist/twcss-to-sass.js examples/node/ && node examples/node/example.js",
    "build": "webpack && npm run build:node && npm run build:browser",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "himalaya": "^1.1.0",
    "js-beautify": "^1.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.5",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "babel-loader": "^8.1.0",
    "cpx": "^1.5.0",
    "husky": "^4.2.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
